{
  "name": "just-dance-event-hub-backend",
  "version": "1.0.0",
  "description": "Backend API for Just Dance Event Hub - A comprehensive platform for managing Just Dance events, queues, tournaments, and user interactions.",
  "main": "dist/app.js",
  "keywords": ["just-dance", "event-management", "api", "typescript", "express", "websocket"],
  "repository": {
    "type": "git",
    "url": "https://github.com/moisoft/just-dance-event-hub.git"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "setup-db": "node scripts/setup-prod-db.js",
    "websocket": "node websocket-server.js",
    "websocket:dev": "nodemon websocket-server.js",
    "audit:fix": "npm audit fix",
    "prepare": "npm run build",
    "prestart": "npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "unzipper": "^0.10.14",
    "uuid": "^8.3.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "@types/supertest": "^6.0.2",
    "@types/unzipper": "^0.10.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "author": "Just Dance Event Hub Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
